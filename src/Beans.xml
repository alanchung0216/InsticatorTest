<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <bean id="Employee" class="com.insticator.model.Employee">
   </bean>   
   <bean id="Fulltime" class="com.insticator.model.Fulltime" parent="Employee">
   </bean>
   <bean id="Parttime" class="com.insticator.model.Parttime" parent="Employee">
   </bean>
   <bean id="Intern" class="com.insticator.model.Intern" parent="Employee">
   </bean>
   <bean id="Contracter" class="com.insticator.model.Contracter" parent="Employee">
   </bean>  
     
</beans>